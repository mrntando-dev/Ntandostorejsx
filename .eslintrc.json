
        {
          "env": {
            "browser": true,
            "es2021": true,
            "node": true
          },
          "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:react/jsx-runtime",
            "plugin:react-hooks/recommended"
          ],
          "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
          },
          "settings": {
            "react": {
              "version": "18.2"
            }
          },
          "plugins": ["react", "react-hooks"],
          "rules": {
            "react/prop-types": "off"
          }
        }



















